package views

import "go/app/views/components"
import "go/app/views/layout"
import "go/app/views/widgets"

templ Login() {
	@layout.Layout(layout.LayoutProps{
		Title: "Вход на сайт",
		MetaDescription: "Войти на сайт поиска работы",
	}) {
		<main>
			@LoginStyle()
			@layout.Header() {
			@components.Title("Войти на сайт")
			@components.SubTitle("Войдите под своей учётной записью")
			@widgets.LoginForm()
			}
		</main>
	}
}

